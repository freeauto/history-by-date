{
  "name": "History by Day",
  "version": "1",
  "manifest_version": 2,
  "browser_action": {
    "default_icon": "dist/logo_128.png",
    "default_title": "ReplyWiki",
    "default_popup": "front/popup.html"
  },
  "content_security_policy": "default-src 'self'; script-src 'self' http://localhost:3000 'unsafe-eval'; connect-src http://localhost:3000",
  "permissions": [
    "history",
    "storage"
  ],
  "web_accessible_resources": [ "front/*", "dist/*" ],
  "icons": {
    "128": "dist/logo_128.png"
  }
}
